{
    "name": "colly",
    "version": "0.8.2",
    "description": "Open Source Web-based Website URL Collection app",
    "keywords": [
        "web",
        "website",
        "collect",
        "url",
        "manage"
    ],
    "bugs": {
        "url": "https://github.com/jarne/colly/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Jarne",
        "url": "https://github.com/jarne"
    },
    "main": "src/index.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/jarne/colly.git"
    },
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "dev:test-data": "node testDataServer.js",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "mocha --recursive --parallel --require test/fixtures.js",
        "test:ci": "mocha --recursive --exit --require test/fixtures.js --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/mocha/junit.xml",
        "test:coverage": "c8 --all -x client --reporter html pnpm run test",
        "test:insert-test-data": "node insertTestData.js",
        "lint": "eslint src",
        "lint:ci": "eslint --format junit --output-file ./lint-results/eslint/junit.xml src",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,html,css,scss,yml,md}\"",
        "init:user": "node userInit.js"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.511.0",
        "@aws-sdk/s3-request-presigner": "^3.511.0",
        "bcrypt": "^6.0.0",
        "cookie-parser": "^1.4.7",
        "dotenv": "^17.0.0",
        "express": "^5.0.0",
        "jsonwebtoken": "^9.0",
        "metascraper": "^5.43.4",
        "metascraper-description": "^5.43.4",
        "metascraper-image": "^5.43.4",
        "metascraper-logo": "^5.43.4",
        "metascraper-logo-favicon": "^5.43.6",
        "metascraper-title": "^5.43.4",
        "mongoose": "^9.0.0",
        "passport": "^0.7",
        "passport-jwt": "^4.0",
        "passport-local": "^1.0",
        "sharp": "^0.34.0",
        "uuid": "^13.0.0",
        "validator": "^13.11",
        "winston": "^3.11.0",
        "winston-loki": "^6.0.8",
        "zod": "^4.0.15"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.0",
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/node": "^25.2.1",
        "@types/passport": "^1.0.17",
        "@types/passport-jwt": "^4.0.1",
        "@types/passport-local": "^1.0.38",
        "@types/validator": "^13.15.10",
        "c8": "^10.0.0",
        "chai": "^6.0.0",
        "cors": "^2.8",
        "eslint": "^10.0.0",
        "eslint-formatter-junit": "^9.0.0",
        "eslint-plugin-jsdoc": "^62.0.0",
        "globals": "^17.0.0",
        "mocha": "^11.0.0",
        "mocha-junit-reporter": "^2.2",
        "morgan": "^1.10",
        "nodemon": "^3.0",
        "prettier": "^3.2",
        "qs": "^6.14.0",
        "supertest": "^7.0.0",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0"
    },
    "type": "module",
    "packageManager": "pnpm@10.30.1+sha512.3590e550d5384caa39bd5c7c739f72270234b2f6059e13018f975c313b1eb9fefcc09714048765d4d9efe961382c312e624572c0420762bdc5d5940cdf9be73a"
}
